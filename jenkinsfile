pipeline {
    agent { lable 'jdk_8' }
      stages {
        stage ( 'vcs' ) {
            steps {
                git url: 'https://github.com/TEJA79955/game-of-life.git',
                branch: 'declarative'
            }
        }
        stage ( 'compile' ) {
            steps {
                sh 'mvn compile'
            }
        }
    }  
}    
